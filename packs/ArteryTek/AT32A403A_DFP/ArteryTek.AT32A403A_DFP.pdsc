  <conditions>
    <!-- Compiler Conditions -->
    <condition id="Compiler ARMCC">
      <require Tcompiler="ARMCC"/>
    </condition>

    <!-- Device Conditions -->
    <condition id="AT32A403Axx">
      <description>ArteryTek AT32A403A Devices</description>
      <require Dvendor="ArteryTek:143" Dname="-AT32A403A*"/>
      <require Cclass="Device" Cgroup="Startup" />
    </condition>

    <condition id="AT32A403A">
      <description>ArteryTek AT32A403A Devices</description>
      <accept condition="AT32A403Axx"/>
    </condition>

    <condition id="AT32A403Axx ARMCC">
      <description>ArteryTek AT32A403Axx Series devices</description>
      <require Dvendor="ArteryTek:143" Dname="-AT32A403A*"/>
      <require condition="Compiler ARMCC"/>
    </condition>

    <!-- Device + CMSIS Conditions -->
    <condition id="AT32A403A CMSIS">
      <description>ArteryTek AT32A403A Devices and CMSIS-CORE</description>
      <require condition="AT32A403A"/>
      <require Cclass="CMSIS" Cgroup="CORE"/>
      <require Cclass="Device" Cgroup="at32a403a_conf"/>    
      <require Cclass="Device" Cgroup="at32a403a_firmware_library" Csub="crm"/>           
    </condition>

    <!-- AT32A403A STDPERIPHERALS CRM Conditions -->
    <condition id="AT32A403A STDPERIPHERALS CRM">
      <description>ArteryTek AT32A403A Standard Peripherals Drivers with CRM</description>
      <require condition="AT32A403A CMSIS"/>
      <require Cclass="Device" Cgroup="at32a403a_firmware_library" Csub="crm"/>
    </condition>

     <!-- AT32A403A STDPERIPHERALS MISC Conditions -->
    <condition id="AT32A403A STDPERIPHERALS MISC">
      <description>ArteryTek  AT32A403A Standard Peripherals Drivers with MISC</description>
      <require condition="AT32A403A CMSIS"/>
      <require Cclass="Device" Cgroup="at32a403a_firmware_library" Csub="misc"/>
    </condition>

     <!-- AT32A403A STDPERIPHERALS EVAL Conditions -->
    <condition id="AT32A403A STDPERIPHERALS EVB">
      <description>ArteryTek AT32A403A Standard Peripherals Drivers with EVB</description>
      <require condition="AT32A403A CMSIS"/>
      <require Cclass="Device" Cgroup="at32a403a_firmware_library" Csub="gpio"/>
      <require Cclass="Device" Cgroup="at32a403a_firmware_library" Csub="exint"/>
      <require Cclass="Device" Cgroup="at32a403a_firmware_library" Csub="usart"/>
    </condition>
  </conditions>

